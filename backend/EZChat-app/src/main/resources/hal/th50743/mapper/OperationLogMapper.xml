<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="hal.th50743.mapper.OperationLogMapper">

    <insert id="insertLog" parameterType="hal.th50743.pojo.OperationLog" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO operation_log (
            user_id,
            module,
            type,
            content,
            ip,
            create_time
        ) VALUES (
            #{userId},
            #{module},
            #{type},
            #{content},
            #{ip},
            #{createTime}
        )
    </insert>

</mapper>
